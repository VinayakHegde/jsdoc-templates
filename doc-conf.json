{
    "opts": {
        "encoding": "utf8",
        "recurse": true 
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [ "<drive>:path/to/source/code/folder" ],
        "exclude": [ "<drive>:path/to/source/code/folder/to/be/excluded" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [ "jsdoc", "closure" ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "name": "App Name",
        "useCollapsibles": true,
        "logo": {
            "url": "../images.png",
            "width": "200px",
            "height": "auto",
            "link": ""
        },
        "tabNames": {
            "api": "API",
            "tutorials": "Examples"
        }
    }
  }
  